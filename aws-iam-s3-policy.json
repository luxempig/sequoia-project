{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "SequoiaCanonicalBucketAccess",
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket",
                "s3:GetObject",
                "s3:GetBucketLocation"
            ],
            "Resource": [
                "arn:aws:s3:::sequoia-canonical",
                "arn:aws:s3:::sequoia-canonical/*"
            ]
        },
        {
            "Sid": "SequoiaMediaBucketAccess",
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket",
                "s3:GetObject",
                "s3:GetBucketLocation"
            ],
            "Resource": [
                "arn:aws:s3:::uss-sequoia-bucket",
                "arn:aws:s3:::uss-sequoia-bucket/*"
            ]
        },
        {
            "Sid": "SequoiaPublicBucketAccess",
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket",
                "s3:GetObject",
                "s3:GetBucketLocation"
            ],
            "Resource": [
                "arn:aws:s3:::sequoia-public",
                "arn:aws:s3:::sequoia-public/*"
            ]
        }
    ]
}